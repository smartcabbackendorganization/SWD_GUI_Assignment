<!-- Based on the default autogenerated ASP NET application with Identity framework -->
@using WebApplication1.Models
@using Microsoft.AspNet.Identity
@model WebApplication1.Models.RegistrerSensorViewModel
@{
    ViewBag.Title = "Home Page";
}
<div class="row">
    <div class="col-md-4">

        @if (Request.IsAuthenticated)
        {
            <div id="SensorApp">
                <label>Filter by</label>
                <input v-model="filter" type="text" id="FilterLokation" />
                <div>
                    <b-table :filter="filter" striped hover :fields="['Sensorid','LokationsId','CreatedBy','Træart','Lat', 'Lon']" :items="sensors"></b-table>
                </div>
            </div>
        }
    </div>
</div>
@section Scripts {
    <script src="~/js/sensor.js"></script>
}